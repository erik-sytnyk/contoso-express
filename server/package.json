    "bcrypt-nodejs": "0.0.3",
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "compression": "1.7.1",
    "config": "1.29.0",
    "connect-flash": "0.1.1",
    "cookie-parser": "1.4.3",
    "cookie-session": "2.0.0-beta.3",
    "cors": "2.8.4",
