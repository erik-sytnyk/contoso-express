{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "serve": "nodemon --exec ts-node -- ./src/index.ts",
    "format": "prettier \"src/**/*.{ts,js,json,css}\" --write"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "bracketSpacing": false
  },
  "globalDependencies": {
    "typescript": "2.2.1"
  },
  "dependencies": {
    "@types/mocha": "8.2.1",
    "@types/node": "14.14.31",
    "app-module-path": "2.2.0",
    "body-parser": "1.18.2",
    "compression": "1.7.1",
    "config": "1.29.0",
    "cookie-parser": "1.4.3",
    "cookie-session": "2.0.0-beta.3",
    "cors": "2.8.4",
    "es6-template-strings": "2.0.1",
    "express": "4.16.2",
    "fs-extra": "5.0.0",
    "joi": "13.0.2",
    "lodash": "4.17.21",
    "moment": "2.20.1",
    "morgan": "1.10.0",
    "pg": "8.5.1",
    "sequelize": "6.5.0",
    "sqlite3": "5.0.2",
    "winston": "2.4.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "mocha": "4.1.0",
    "ts-node": "9.1.1",
    "tslint": "5.8.0"
  }
}
